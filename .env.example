# Variáveis de ambiente para o Password Authenticator

# Banco de dados MongoDB
MONGODB_URI=mongodb://localhost:27017/password-authenticator

# JWT Secret - Use uma string aleatória e segura
JWT_SECRET=seu_jwt_secret_muito_seguro_aqui_pelo_menos_32_caracteres

# Chave de criptografia AES-256-GCM (32 bytes)
ENCRYPTION_KEY=sua_chave_de_criptografia_32_bytes_aqui

# Porta do servidor
PORT=5000

# Ambiente
NODE_ENV=development

# URL do frontend (para CORS)
CLIENT_URL=http://localhost:3000